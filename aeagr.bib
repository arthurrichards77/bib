@CONFERENCE{R08,
  author = {Richards, A. G.},
  title = {Adaptive Navigation in an Uncertain Urban Environment},
  booktitle = {Proceedings of the Conference of the Systems Engineering for Autonomous
	Systems Defence Technology Centre},
  year = {2008},
  address = {Edinburgh},
  month = {June},
  abstract = {The concept of Adaptive Navigation has been developed, combining Simultaneous
	Localisation and Mapping (SLAM) and Receding Horizon Control (RHC)
	to solve the problem of navigation in an uncertain, GPS-denied, urban
	environment. Both SLAM and RHC have been modified to work with a
	representation of the environment using walls. RHC has been further
	modified to account for uncertainty in the wall locations. An initial
	implementation in a simulated challenge scenario has shown that Adaptive
