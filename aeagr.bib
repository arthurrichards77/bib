@CONFERENCE{R10,
  author = {Richards, A. G.},
  title = {Smart Loitering for {UAV} Collision Avoidance},
  booktitle = {Bristol International UAV Systems Conference},
  year = {2010},
  abstract = {A new guidance approach, dubbed ?Smart Loitering?, is developed, intended
