@CONFERENCE{DR09,
  author = {Deittert, M. and Richards, A.G.},
  title = {Efficient Wall Avoidance Methods for Adaptive Navigation},
  booktitle = {Proceedings of the Conference of the Systems Engineering for Autonomous
	Systems Defence Technology Centre},
  year = {2009},
  address = {Edinburgh},
  month = {July},
  abstract = {Adaptive navigation is a novel combination of receding horizon control
	and simultaneous locating and mapping. Experience has shown the initial
	implementation of adaptive navigation to be computationally expensive,
	due to the large size of the path planner's optimisation problem,
	as well as node to cell visibility checking. Successful avoidance
	of walls and other obstacles requires the ability to avoid known
	obstacles at the path planning stage, beside preventing the path
	from leading into areas with yet undetected obstacles. This paper
	presents two new approaches to achieving wall avoidance behaviour
	in the trajectory planner, which substantially reduce the size of
	the optimisation problem and improve path planning speed. Furthermore,
	a new strategy of path planning node placement is introduced, which
	signicantly reduces computation by dispensing with exhaustive visibility
	checking. The new approaches show a marked improvement in computation
	time when compared to the initial implementation, as well as improved
	success rates.},
  owner = {aeagr},
  timestamp = {2009.08.06}
}

@ARTICLE{DRTP09,
  author = {Deittert, M. and Richards, A. and Toomer, C.A. and Pipe, A.},
  title = {Engineless UAV Propulsion by Dynamic Soaring},
  journal = {AIAA Journal of Guidance, Control and Dynamics},
  year = {2009},
  volume = {32},
  pages = {1446--1457},
  number = {5},
  month = {September},
  abstract = {Dynamic soaring is a flight technique which extracts energy from wind
	gradients with the potential to power small UAVs in maritime applications.
	Wind gradients of the required magnitude naturally occur at the air-sea
	interface due to friction between the waves and the moving air. Suitability
	of dynamic soaring as a means of propulsion requires clarification
	of the achievable flight performance and the likelihood of favourable
	winds. Optimal trajectories for minimal and maximal wind conditions
	are generated as well as trajectories for optimal cross-country travel.
	The flight model?s differential flatness property is used to simplify
	the optimisation problem. The likelihood of favourable winds is predicted
	based on long term weather statistics and knowledge of the minimal
	and maximal permissible wind strength. Comparison of the likelihood
	of favourable winds for the wandering albatross and a UAV of similar
	size shows that the ability to fly close to the surface is a key
	factor governing dynamic soaring performance.},
  owner = {aeagr},
  timestamp = {2009.08.06}
}

@CONFERENCE{DRM10,
  author = {Deittert, M. and Richards, A. G. and Mathews, G.},
  title = {Receding Horizon Control in Unknown Environments: Experimental Results},
  booktitle = {IEEE International Conference on Robotics and Automation (ICRA)},
  year = {2010},
  address = {Anchorage, Alaska},
  month = {May},
  abstract = {This paper presents a novel approach to navigation in an a priori
	unknown, GPS-denied environment. The aim is to combine dynamic path
	planning with the ability to learn about the environment. The vehicle
	is tasked with autonomous travel from an uncertain initial position
	to an uncertain target, without prior mapping information. The environment
	is modelled using linear segments that represent boundaries between
	the estimated traversable and nontraversable regions. The approach
	integrates Receding Horizon Control (RHC) and Simultaneous Localisation
	and Mapping (SLAM). The control problem is formulated as a mixed
	integer linear program (MILP) and explicitly includes the obstacles
	and vehicle dynamics.We present the results of our experiments using
	Pioneer robots, as well as, simulation results which clearly demonstrated
	the impact of each component of the system.},
  owner = {aeagr},
  timestamp = {2010.02.01}
}

@CONFERENCE{DRM10a,
  author = {Deittert, M. and Richards, A. G. and Mathews, G.},
  title = {Adaptive Navigation in Cluttered Environments with Wheel-Steered
	Vehicles},
  booktitle = {5th SEAS DTC Technical Conference},
  year = {2010},
  address = {Edinburgh},
  abstract = {Adaptive Navigation is a novel approach to navigation in an a priori
	unknown, GPS-denied environment. The aim is to combine dynamic path
	planning with the ability to learn about the environment. The vehicle
	is tasked with autonomous travel from an uncertain initial position
	to an uncertain target, without prior mapping information. This paper
	presents progress since the initial demonstration on a Pioneer platform,
	in particular, transition to the UoB Rover platform, i.e. from a
	slow skid-steered vehicle to a faster vehicle with wheel steering,
	and a transition from a small straight-lined world to a larger sensor
