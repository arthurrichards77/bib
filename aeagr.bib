@Conference{DR09,
  Title                    = {Efficient Wall Avoidance Methods for Adaptive Navigation},
  Author                   = {Deittert, M. and Richards, A.G.},
  Booktitle                = {Proceedings of the Conference of the Systems Engineering for Autonomous Systems Defence Technology Centre},
  Year                     = {2009},

  Address                  = {Edinburgh},
  Month                    = {July},

  Abstract                 = {Adaptive navigation is a novel combination of receding horizon control and simultaneous locating and mapping. Experience has shown the initial implementation of adaptive navigation to be computationally expensive, due to the large size of the path planner's optimisation problem, as well as node to cell visibility checking. Successful avoidance of walls and other obstacles requires the ability to avoid known obstacles at the path planning stage, beside preventing the path from leading into areas with yet undetected obstacles. This paper presents two new approaches to achieving wall avoidance behaviour in the trajectory planner, which substantially reduce the size of the optimisation problem and improve path planning speed. Furthermore, a new strategy of path planning node placement is introduced, which signicantly reduces computation by dispensing with exhaustive visibility checking. The new approaches show a marked improvement in computation time when compared to the initial implementation, as well as improved success rates.},
  Owner                    = {aeagr},
  Timestamp                = {2009.08.06}
}

@Article{DRTP09,
  Title                    = {Engineless UAV Propulsion by Dynamic Soaring},
  Author                   = {Deittert, M. and Richards, A. and Toomer, C.A. and Pipe, A.},
  Journal                  = {AIAA Journal of Guidance, Control and Dynamics},
  Year                     = {2009},

  Month                    = {September},
  Number                   = {5},
  Pages                    = {1446--1457},
  Volume                   = {32},

  Abstract                 = {Dynamic soaring is a flight technique which extracts energy from wind gradients with the potential to power small UAVs in maritime applications. Wind gradients of the required magnitude naturally occur at the air-sea interface due to friction between the waves and the moving air. Suitability of dynamic soaring as a means of propulsion requires clarification of the achievable flight performance and the likelihood of favourable winds. Optimal trajectories for minimal and maximal wind conditions are generated as well as trajectories for optimal cross-country travel. The flight model?s differential flatness property is used to simplify the optimisation problem. The likelihood of favourable winds is predicted based on long term weather statistics and knowledge of the minimal and maximal permissible wind strength. Comparison of the likelihood of favourable winds for the wandering albatross and a UAV of similar size shows that the ability to fly close to the surface is a key factor governing dynamic soaring performance.},
  Owner                    = {aeagr},
  Timestamp                = {2009.08.06}
}

@Conference{DRM10,
  Title                    = {Receding Horizon Control in Unknown Environments: Experimental Results},
  Author                   = {Deittert, M. and Richards, A. G. and Mathews, G.},
  Booktitle                = {IEEE International Conference on Robotics and Automation (ICRA)},
  Year                     = {2010},

  Address                  = {Anchorage, Alaska},
  Month                    = {May},

  Abstract                 = {This paper presents a novel approach to navigation in an a priori unknown, GPS-denied environment. The aim is to combine dynamic path planning with the ability to learn about the environment. The vehicle is tasked with autonomous travel from an uncertain initial position to an uncertain target, without prior mapping information. The environment is modelled using linear segments that represent boundaries between the estimated traversable and nontraversable regions. The approach integrates Receding Horizon Control (RHC) and Simultaneous Localisation and Mapping (SLAM). The control problem is formulated as a mixed integer linear program (MILP) and explicitly includes the obstacles and vehicle dynamics.We present the results of our experiments using Pioneer robots, as well as, simulation results which clearly demonstrated the impact of each component of the system.},
  Owner                    = {aeagr},
  Timestamp                = {2010.02.01}
}

@Conference{DRM10a,
  Title                    = {Adaptive Navigation in Cluttered Environments with Wheel-Steered Vehicles},
  Author                   = {Deittert, M. and Richards, A. G. and Mathews, G.},
  Booktitle                = {5th SEAS DTC Technical Conference},
  Year                     = {2010},

  Address                  = {Edinburgh},

  Abstract                 = {Adaptive Navigation is a novel approach to navigation in an a priori unknown, GPS-denied environment. The aim is to combine dynamic path planning with the ability to learn about the environment. The vehicle is tasked with autonomous travel from an uncertain initial position to an uncertain target, without prior mapping information. This paper presents progress since the initial demonstration on a Pioneer platform, in particular, transition to the UoB Rover platform, i.e. from a slow skid-steered vehicle to a faster vehicle with wheel steering, and a transition from a small straight-lined world to a larger sensor rich and cluttered environment. Algorithm modifications to suit the new environment and platform are discussed and initial qualitative results are presented.},
  Owner                    = {aeagr},
  Timestamp                = {2010.09.17}
}

@Conference{DRTP09a,
  Title                    = {Dynamic Soaring Flight in Turbulence},
  Author                   = {Deittert, M. and Richards, A. G. and Toomer, C. and Pipe, A.},
  Booktitle                = {Proceedings of the AIAA Guidance Navigation and Control Conference},
  Year                     = {2009},

  Address                  = {Chicago, IL},
  Month                    = {August},
  Number                   = {AIAA 2009-6012},
  Organization             = {AIAA},

  Abstract                 = {Dynamic soaring is an engineless flight technique which extracts energy from wind gradients and has potential to enable long duration flight for small UAVs. For this paper, the authors investigate how atmospheric turbulence affects the dynamic soaring flight of a generic 3 meter wing span UAV. A previous publication established outer bounds of 7.6 m/s and 32 m/s on the range of feasible wind speed while assuming perfect control. The authors propose a periodic LQR to reject disturbances arising from atmospheric turbulence and modeling errors. Simulations with atmospheric turbulence, investigate feasibility of controlled dynamic soaring flight for wind speeds between 8 m/s and 24 m/s. Comparison between engineless and engine assisted dynamic soaring flight illustrates the importance of controlling the UAV?s energy in contrast to focusing on rejecting trajectory errors.},
  Owner                    = {aeagr},
  Timestamp                = {2009.09.04}
}

@InProceedings{DOC10,
  Title                    = {Depth estimation for autonomous robot navigation: A comparative approach},
  Author                   = {Diamantas, S.C. and Oikonomidis, A. and Crowder, R.M.},
  Booktitle                = {Imaging Systems and Techniques (IST), 2010 IEEE International Conference on},
  Year                     = {2010},
  Month                    = {july},
  Pages                    = {426 -430},

  Abstract                 = {Depth estimation has long been a fundamental problem both in robotics science and in computer vision. Various methods have been developed and implemented in a large number of applications. Despite the rapid progress in the field the last few years, computation remains a significant issue of the methods employed. In this work, we have implemented two different strategies for inferring depth, both of which are computationally efficient. The first one is inspired by biology, that is optical flow, while the second one is based on a least squares method. In the first strategy, we observe the length variation of the optic flow vectors of a landmark at varying distances and velocities. In the second strategy, we take snapshots of a landmark from different positions and use a least squares approach to estimate the distance between the robot and a landmark. An evaluation of the two different strategies for various depth estimations has been deployed and the results are presented in this paper.},
  Doi                      = {10.1109/IST.2010.5548483},
  Keywords                 = {Biomedical optical imaging;Image motion analysis;Intelligent agent;Intelligent robots;Least squares approximation;Least squares methods;Mobile robots;Navigation;Optical sensors;Robot vision systems;image sequences;least squares approximations;mobile robots;path planning;robot vision;autonomous robot navigation;computer vision;depth estimation;least squares approach;least squares method;optic flow vectors;optical flow;robotics science;depth estimation;least squares;mobile robotics;optical flow;visual navigation;},
  Owner                    = {Arthur},
